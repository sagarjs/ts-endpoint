{
  "name": "ts-endpoint",
  "version": "0.1.0",
  "description": "A sample TS endpoint",
  "main": "server.ts",
  "scripts": {
    "prestart": "tsoa swagger-and-routes",
    "predebug": "tsoa swagger-and-routes",
    "start": "nodemon src/server.ts",
    "debug": "nodemon --exec node --inspect=0.0.0.0:9229 --require ts-node/register src/server.ts",
    "generate": "tsoa swagger-and-routes",
    "test": "jest --forceExit --detectOpenHandles --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.9.16",
    "mongoose-paginate": "^5.0.3",
    "nodemon": "^2.0.4",
    "spdy": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.10.1",
    "tsoa": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.21",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^14.0.5",
    "@types/spdy": "^3.4.4",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "jest": "^26.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0"
  },
  "author": "Sagar Sodah",
  "license": "MIT"
}
